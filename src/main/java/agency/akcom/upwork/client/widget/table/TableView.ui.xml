<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
             xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">
    <b:Panel>
        <b:PanelHeader>
            <b:Heading size="H3" text="Search Works"/>
        </b:PanelHeader>
        <b:PanelBody>
            <b.html:ClearFix addStyleNames="margin"/>

            <b.gwt:CellTable width="100%" bordered="true" condensed="true" striped="true" hover="true"
                             ui:field="userTable" />
            <b:Pagination ui:field="userTablePagination" />
        </b:PanelBody>
        <b:PanelFooter>
            <b.html:Strong text="PanelFooter"/>
        </b:PanelFooter>
    </b:Panel>
</ui:UiBinder>
